			<% Habilitation.order(:name).each do |habilitation| %>
			<a class="popup-modal edit_checklist" href="#modal_edit_habilitation_<%=habilitation.id%>">Editar nome</a>
			<%= render 'biddings/modal_edit/modal_edit_habilitation', object: habilitation %>
			<% end %>
		</div>
		<% end %>
		<div class="col-xs-9">	
			<%= f.association :habilitations, collection: Habilitation.order(:name), label_method: lambda { |object| "#{object.name.mb_chars.upcase}"} , value_method: :id, label: false, as: :check_boxes, disabled: @current_user.disabled_edit_read_bidding? %>
