<hr class="drop-line">

<br>

<div class="content no-margin">
	<div class="row">
		<div class="col-xs-12">
			<div class="title slide_historic_observations_3 show_content_3">
				<span>Visualizar Histórico de Observações (<%=(f.object.observations.select{|item| item[:observation_type] == 3}.length-1)%>) <span class="glyphicon glyphicon-menu-down" aria-hidden="true"></span></span>
			</div>
		</div>
	</div>
</div>

<!-- Histórico de Observações -->
<%= f.fields_for :observations, f.object.observations.select{|item| item[:observation_type] == 3}, defaults: { input_html: { class: 'form-control' } } do |observation| %>
<% if observation.object.persisted? %>
<div class="row row_with_observation_3 observation-div">
	<div class="col-xs-3">
		<label for="#"><%= Observation.human_attribute_name(:created_at) %>:</label>
		<% if observation.object.created_at %>
		<%= I18n.l observation.object.created_at.to_datetime, format: :full %>
		<% end %>
	</div>
	<div class="col-xs-3">
		<label for="#"><%= Observation.human_attribute_name(:user)%>:</label>
		<% if observation.object.user %>
		<%= observation.object.user.name %>
		<% end %>
	</div>
	<div class="col-xs-4">
		<label for="#"><%= Observation.human_attribute_name(:observation)%>:</label>
		<% if observation.object.user_id == @current_user.id %>
		<%= observation.input :observation, label: false, disabled: @current_user.disabled_edit_read_bidding? %>
		<% else %>
		<% if observation.object.observation %>
		<%= observation.object.observation %>
		<% end %>
		<% end %>
	</div>
	<% if !@current_user.disabled_edit_read_bidding? %>
	<div class="col-xs-2">
		<a title="Excluir" data-confirm="Tem certeza que deseja excluir essa observação?" rel="nofollow" data-method="delete" href="<%= delete_observation_bidding_path(observation.object) %>">Excluir
		</a>
	</div>
	<% end %>
</div>
<% end %>
<% end %>
<!-- Fim histórico de observações -->

<!-- Nova observação -->
<%= f.fields_for :observations, f.object.observations.select{|item| item[:observation_type] == 3}, defaults: { input_html: { class: 'form-control' } } do |observation| %>
<% if !observation.object.persisted? %>
<div class="row">
	<div class="col-xs-12">
		<label for="#"><%= Bidding.human_attribute_name(:catch_organ_notification_observation)%>:</label>
		<%= observation.association :user, :as => :hidden, input_html: { value: @current_user.id}, disabled: @current_user.disabled_edit_read_bidding? %>
		<%= observation.input :observation_type, :as => :hidden, input_html: { value: 3}, disabled: @current_user.disabled_edit_read_bidding? %>
		<%= observation.input :observation, label: false, disabled: @current_user.disabled_edit_read_bidding? %>
	</div>
</div>
<% end %>
<% end %>
<!-- Fim nova observação -->

<br>
<% if !@current_user.disabled_edit_read_bidding? %>
<%= f.submit 'Salvar', :class => 'save-btn', :name => "save_read" %>
<% end %>